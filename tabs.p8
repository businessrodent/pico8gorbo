function _init()
	global.title="gorbo"
	global.gold=80
	global.length=0
	global.boxes={}
	global.xlim=0
	global.kidneys=2
	gorbo_init()
	gamestart=true
	life=3
end

function _update()
	crsr:update()
	gorbo_life()
	if global.title=="shop" then
		shop_update()
	elseif global.title=="sell" then
		sell_update()
	elseif global.title=="gorbo" then
		gorbo_update()
	elseif global.title=="inventory" then
		inventory_update()
	end
	for b in all(global.boxes) do
		b:update()
	end
end

function _draw()
	if gamestart then
		cls()
		gorbo_draw()
		titledraw()
		spr(016,121,104)
		print("gold:"..global.gold,
			127-25-(#tostr(global.gold)*4),
			106
		)
		for b in all(boxes) do
			b:draw()
		end
	else
		dead()
	end
end
