inventory=class:new({
	inv={item:new()},
	
	tolist=function(_ENV)
		global.boxes={}
		for x=1,#inv do
			add(global.boxes,box:new({
				y=x-1,
				offset=
				12+(x-1)*(box.height+1),
				text=inv[x]:listbox()
			}))
		end
		global.length=#boxes+1
		add(boxes,box:new({
			y=#boxes,
			offset=95,
			toffy=6,
			toffx=25,
			text={"back"}
		}))
	end,
	
	itemadd=function(_ENV, it)
		it = it or item:new()
		c=0
		for x in all(inv) do
			if x.name==it.name then
				c=1
				x.nom+=1
			end
		end
		if c==0 then
			t=item:new({
				name=it.name,
				val=it.val,
				nom=1,
				desc=it.desc,
			})
			add(inv,t)
		end
	end,
	
	itemsub=function(_ENV, it)
		for x in all(inv) do
			if x.name==it.name then
				if x.nom==1 then
					del(inv,x)
				else
					x.nom-=1
				end
			end
		end
	end
})

