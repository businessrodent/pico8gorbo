gorbo_update=function()
	if btnp(4,0) then
		if crsr.x==0 then
			shop_init()
		elseif crsr.x==1 then
			sell_init()
		elseif crsr.x==2 then
			inventory_init()
		end
	end
end


shop_init=function()
	crsr.y=0
	crsr.x=0
	global.xlim=0
	global.title="shop"
end

shop_update=function()
	shop:tolist()
	if btnp(4,0) then
		if crsr.y < #global.boxes-1 then		
			if global.gold
				>=shop.inv[crsr.y+1].val then
					player:itemadd(
						shop.inv[crsr.y+1]
						)
					global.gold-=
					 shop.inv[crsr.y+1].val
					shop:itemsub(
						shop.inv[crsr.y+1]
					)
			end
		elseif 
		crsr.y==#global.boxes-1 then
			gorbo_init()
		end
	elseif btnp(5,0) then
		gorbo_init()
	end
end

sell_init=function()
	global.title="sell"
	crsr.y=0
	crsr.x=0
	global.xlim=0
end

sell_update=function()
	player:tolist()
	if btnp(4,0) then
		if crsr.y < #global.boxes-1 then
			shop:itemadd(
				player.inv[crsr.y+1]
				)
			if player.inv[crsr.y+1].name== "gorbo's kidney" then
				if player.inv[crsr.y+1].nom==2 then
					global.gold+=
						3*player.inv[crsr.y+1].val
				elseif player.inv[crsr.y+1].nom==1 then
					global.gold+=
						2*player.inv[crsr.y+1].val
				end
			else
				global.gold+=
					flr(player.inv[crsr.y+1].val/2)
			end
			player:itemsub(
				player.inv[crsr.y+1]
			)
		elseif 
		crsr.y==#global.boxes-1 then
			gorbo_init()
		end
	elseif btnp(5,0) then
		gorbo_init()
	end
end

inventory_init=function()
	global.title="inventory"
	crsr.x=0
	crsr.y=0
	global.xlim=0
	focus=0
end

inventory_update=function()
	if focus==0 then
		player:tolist()
	end
	if btnp(4,0) then
		if crsr.y<#global.boxes-1 then
			focus=1
			global.boxes={
				box:new({
					height=82,
					text=player.inv[crsr.y+1]:focusbox()
				}),
				box:new({
					y=1,
					offset=95,
					toffy=6,
					toffx=25,
					text={"back"}
				})
			}
		elseif 
		crsr.y==#global.boxes-1 then
			if focus==1 then
				focus=0
				crsr.y=0
			else
				gorbo_init()
			end
		end
	elseif btnp(5,0) then
		if focus==1 then
			crsr.y=0
			focus=0
		else
			gorbo_init()
		end
	end
end

gorbo_life=function()
	c=0
	for k in all(shop.inv) do
		if k.name=="gorbo's kidney" then
			c=1
			if k.nom==2 then
				global.kidneys=2
			elseif k.nom==1 then
				global.kidneys=1
			end
		end
	end
	if c==0 then
	 global.kidneys=0
	end
	if l==90 then
		l=0
		if global.kidneys==0 then
			life-=1
		elseif global.kidneys==2 then
			if life < 3 then
				life+=1
			end
		end
	end
	if life==0 then
		cls()
		life=-1
		gorbo_draw()
		gamestart=false
		dead()
	end
	l+=1
end
