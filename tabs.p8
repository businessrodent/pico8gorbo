box=entity:new({
	text={"cum", "it's cum"},
	clr=6,
	x=0,
	y=0,
	height=15,
	width=68,
	offset=12,
	xffset=0,
	toffy=3,
	toffx=4,
	vis=1,
	
	update=function(_ENV)
		if x==crsr.x
		and y==crsr.y then
			clr=7
		else
		 clr=13
		end
	end,
	
	draw=function(_ENV)
		if vis==1 then
			rect(
				2+(x*width)+xffset,
				offset,
			 width+(x*width)+xffset,
				offset+height-1,
				clr
			)
			for i=1,#text do
				print(
				 text[i],
					toffx+(x*width)+xffset,
					offset+toffy+(i-1)*6-1,
					clr
				)
			end
		end
	end
})

